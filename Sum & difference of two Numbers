#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int a=0,b=0,sum,diff;
    float a1=0,b1=0,sum1,diff1;
    scanf("%d%d",&a,&b);
    scanf("%f%f",&a1,&b1);
    sum=a+b;
    diff=a-b;
    sum1=a1+b1;
    diff1=a1-b1;
    printf("%d %d\n",sum,diff);
    printf("%.1f %.1f",sum1,diff1);
    
    return 0;
}
